{
  "name": "piikki-client-tg",
  "version": "0.0.5",
  "description": "Telegram client for piikki",
  "main": "src/index.js",
  "engines": {
    "node": "8"
  },
  "scripts": {
    "start": "node build/index.js",
    "build": "tsc -p tsconfig.json --listEmittedFiles",
    "dev": "nodemon --inspect -r dotenv/config -r ts-node/register src/index.ts",
    "test": "npm run lint && mocha test/**/*_spec.ts",
    "test:watch": "mocha -w --inspect test/**/*_spec.ts",
    "lint": "tsc -p tsconfig.json --noEmit && tslint -p tsconfig.json",
    "migrate": "knex migrate:latest",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/majori/piikki-client-tg.git"
  },
  "keywords": [
    "Telegram"
  ],
  "author": "Antti Kivim√§ki",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/majori/piikki-client-tg/issues"
  },
  "homepage": "https://github.com/majori/piikki-client-tg#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "knex": "^0.14.6",
    "lodash": "^4.17.5",
    "pg": "^7.4.1",
    "telegraf": "^3.20.0",
    "telegraf-command-parts": "^1.0.3",
    "winston": "^2.4.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/lodash": "^4.14.106",
    "@types/mocha": "^5.2.0",
    "@types/node": "^9.6.2",
    "@types/sinon": "^4.3.1",
    "@types/winston": "^2.3.9",
    "chai": "^4.1.2",
    "dotenv": "^5.0.1",
    "mocha": "^5.1.1",
    "nodemon": "^1.17.3",
    "sinon": "^4.5.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1"
  }
}
