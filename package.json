  "dependencies": {
    "axios": "^0.18.0",
    "lodash": "^4.17.5",
    "telegraf": "^3.20.0",
    "telegraf-command-parts": "^1.0.3",
    "winston": "^2.4.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.106",
    "@types/node": "^9.6.2",
    "@types/winston": "^2.3.9",
    "dotenv": "^5.0.1",
    "nodemon": "^1.17.3",
